{
  "require": {
    "phpoffice/phpspreadsheet": "^3.3"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.11",
    "wp-coding-standards/wpcs": "^3.1",
    "johnpbloch/wordpress-core": "^6.7"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "scripts": {
    "fix-php": "vendor/bin/phpcbf --standard=WordPress --ignore=vendor,node_modules --extensions=php .",
    "check-php": "vendor/bin/phpcs --standard=WordPress --ignore=vendor,node_modules --extensions=php ."
  }
}
